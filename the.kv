    ImageButton:
<MainWidget>:
    id: MainWidget
    BoxLayout:
        id:lefty
        size_hint: 2,1
        orientation: "vertical"
        Button:
            text: "C"
            size_hint: 1, 1

        ScrollView:
            size_hint:1, 10
            name: scroll
            id: scroll
            StackLayout:
                size_hint_y: None
                padding: 3
                height:self.minimum_height
                id:images

    BoxLayout:
        id: layout
        size_hint:5,1
        orientation: "vertical"
        FileChooserListView:
            id:filechooser
            size_hint:1,10
        Button:
            id: select
            text: "Select"
            size_hint: None, None
            size: 75,40
            pos_hint:{"x":0.4}
            on_press: MainWidget.selected(filechooser.files)
            on_press: root.remove()
    BoxLayout:
        id: lists
        size_hint: 2,1

        orientation: "vertical"
        ScrollView:
            id: list3
            size_hint: 1,8
            StackLayout:
                size_hint_y: None
                height:self.minimum_height
                spacing: 5
                id:classes
        BoxLayout:
            size_hint:1,1
            TextInput:
                id:input
                text: "button"
                multiline: False
                on_enter:
                size_hint: 5,1
            Button:
                text: "add"
                size_hint: 1,1
                on_press: MainWidget.getClass(input.text)
        ScrollView:
            size_hint: 1,8
            StackLayout:
                size_hint_y: None
                height:self.minimum_height
                width:self.minimum_width
                orientation: 'lr-tb'
                id:objects